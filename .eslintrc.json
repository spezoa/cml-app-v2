{
  "extends": [
    "next/core-web-vitals",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-no-target-blank": "warn",
    "react/prop-types": "off",
    "@next/next/no-img-element": "off",
    "react/react-in-jsx-scope": "off",
    "react/jsx-uses-react": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}